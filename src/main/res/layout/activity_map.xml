<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false"
    tools:context="MapActivity">

    <WebView
        android:id="@+id/carte"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="355dp"
        android:gravity="center"

        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv_local_icon1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/location"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="198dp"
        android:gravity="center"

        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv_local_icon2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="120dp"
            android:src="@drawable/location"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"

        android:layout_marginBottom="60dp"
        android:gravity="center"

        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv_itinerary"
            android:layout_width="280dp"
            android:layout_height="200dp"
            android:layout_marginLeft="40dp"
            android:src="@drawable/itenerary"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"

        android:layout_marginBottom="100dp"
        android:gravity="center"

        android:orientation="vertical">

        <ImageView
            android:id="@+id/iv_local_icon3"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginRight="68dp"
            android:src="@drawable/location"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="110dp"
        android:gravity="center"
        android:orientation="vertical"

        android:visibility="gone">

        <ImageView
            android:id="@+id/iv_itiner"
            android:layout_width="370dp"
            android:layout_height="200dp"
            android:layout_marginRight="50dp"
            android:src="@drawable/itenerary" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/activity_horizontal_margin"
        android:gravity="center"

        android:orientation="horizontal">


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab_style_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_spacing"
            android:src="@drawable/itinerary"

            app:fabSize="normal" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab_style_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_spacing"
            android:src="@drawable/abc_ic_clear_mtrl_alpha"
            app:fabSize="normal" />


    </LinearLayout>
</RelativeLayout>